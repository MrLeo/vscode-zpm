{
  "name": "zpm",
  "displayName": "zpm",
  "description": "常用的Git操作及代码片段",
  "keywords": [
    "zpm",
    "git",
    "tag",
    "snippets"
  ],
  "version": "1.0.27",
  "publisher": "MrLeo",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.62.0"
  },
  "homepage": "https://xuebin.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/MrLeo/vscode-zpm.git"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onCommand:extension.tag",
    "onCommand:extension.tagRefresh"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.tag",
        "title": "zpm: 创建Tag  🔖 "
      },
      {
        "command": "extension.tagRefresh",
        "title": "zpm: 删除远端不存在的Tag  ♻️ "
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "html",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "css",
        "path": "./snippets/css.json"
      },
      {
        "language": "scss",
        "path": "./snippets/css.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3",
    "ts-loader": "^9.2.5",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "@vscode/test-electron": "^1.6.2",
    "@types/semver": "^7.3.9",
    "@types/fs-extra": "^9.0.13"
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "execa": "^6.0.0",
    "fs-extra": "^10.0.0",
    "semver": "^7.3.5",
    "simple-git": "^2.47.0"
  }
}
