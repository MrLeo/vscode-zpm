{
  "name": "zpm",
  "displayName": "zpm",
  "description": "自动为当前的Git库添加tag",
  "keywords": [
    "zpm",
    "git",
    "tag"
  ],
  "version": "0.0.5",
  "publisher": "MrLeo",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.32.0"
  },
  "homepage": "https://xuebin.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/MrLeo/vscode-zpm.git"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:extension.tag"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.tag",
        "title": "zpm: 创建Tag 🔖 "
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "semver": "^5.6.0",
    "simple-git": "^1.107.0",
    "dayjs": "^1.8.10"
  }
}
