{
  "name": "zpm",
  "displayName": "zpm",
  "description": "常用的Git操作及代码片段",
  "keywords": [
    "zpm",
    "git",
    "tag",
    "snippets"
  ],
  "version": "1.0.12",
  "publisher": "MrLeo",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.41.0"
  },
  "homepage": "https://xuebin.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/MrLeo/vscode-zpm.git"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension",
  "activationEvents": [
    "onCommand:extension.tag",
    "onCommand:extension.tagRefresh"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.tag",
        "title": "zpm: 创建Tag  🔖 "
      },
      {
        "command": "extension.tagRefresh",
        "title": "zpm: 删除远端不存在的Tag  ♻️ "
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "html",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "css",
        "path": "./snippets/css.json"
      },
      {
        "language": "scss",
        "path": "./snippets/css.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode development --watch --info-verbosity verbose",
    "test-compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "@types/semver": "^5.5.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "dayjs": "^1.8.10",
    "execa": "^1.0.0",
    "semver": "^5.6.0",
    "simple-git": "^1.107.0"
  }
}
